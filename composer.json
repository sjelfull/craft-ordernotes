{
    "name": "superbig/craft-ordernotes",
    "description": "Order notes for Commerce",
    "type": "craft-plugin",
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft commerce",
        "commerce",
        "order notes"
    ],
    "support": {
        "docs": "https://github.com/sjelfull/craft-ordernotes/blob/master/README.md",
        "issues": "https://github.com/sjelfull/craft-ordernotes/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Superbig",
            "homepage": "https://superbig.co"
        }
    ],
    "require": {
        "craftcms/cms": "^5.5",
        "craftcms/commerce": "^5.0",
        "php": "^8.2"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main",
        "craftcms/rector": "dev-main",
        "markhuot/craft-pest-core": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "superbig\\ordernotes\\": "src/"
        }
    },
    "extra": {
        "name": "Order Notes",
        "handle": "order-notes",
        "hasCpSettings": true,
        "hasCpSection": false,
        "schemaVersion": "2.0.0",
        "class": "superbig\\ordernotes\\OrderNotes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true,
            "pestphp/pest-plugin": true
        }
    }
}
